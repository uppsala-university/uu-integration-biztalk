'Type name of the class that implements receiving part of the adapter
"InboundTypeName"="BizTalk.Adapter.SBQueue.SBQueueReceiver"

'Path to the assembly that implements receiving part of the adapter
"InboundAssemblyPath"="%BTSCOMMONFILES%\\SBQueueAdapter\\BizTalk.Adapter.SBQueue.dll"

'SSO registration of the adapter framework property page for receive handler
"ReceiveHandlerPropertiesXML"="<CustomProps><AdapterConfig vt=\"8\"/></CustomProps>"

'SSO registration of the adapter framework property page for receive location
"ReceiveLocationPropertiesXML"="<CustomProps><AdapterConfig vt=\"8\"/></CustomProps>"
'============= End of registration for receive side of adapter ==============

"AliasesXML"="<AdapterAliasList><AdapterAlias>sbqueue://</AdapterAlias></AdapterAliasList>"

