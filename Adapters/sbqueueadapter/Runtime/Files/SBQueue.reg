[HKEY_CLASSES_ROOT\CLSID\{615E18EE-7694-4CBD-80EC-3691511ACCF2}]
@="SBQueue adapter"

[HKEY_CLASSES_ROOT\CLSID\{615E18EE-7694-4CBD-80EC-3691511ACCF2}\BizTalk]
@="BizTalk"
'Type of the transport adapter
"TransportType"="SBQueue"

'Constraints define the capabilities of the adapter: e.g send, receive, request-response, etc
"Constraints"=dword:1809

'============= Registration for receive side of adapter ==============
'Adapter framwork property page for receive handler configuration
"InboundProtocol_PageProv"="{2DE93EE6-CB01-4007-93E9-C3D71689A281}"

'Adapter framework property page for receive location configuration
"ReceiveLocation_PageProv"="{2DE93EE6-CB01-4007-93E9-C3D71689A280}"

'Class ID of the class that implements receiving part of adapter
