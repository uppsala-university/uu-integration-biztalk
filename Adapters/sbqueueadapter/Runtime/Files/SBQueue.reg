"PropertyNameSpace"="http://schemas.microsoft.com/BizTalk/2012/Adapter/BrokeredMessage-properties"

'Type name of the class that provides UI for adapter property pages
"AdapterMgmtTypeName"="BizTalk.Adapter.SBQueueAdmin.SBQueueAdapterManagement"

'Path to the assembly that implments UI for adapter property pages
"AdapterMgmtAssemblyPath"="%BTSCOMMONFILES%\\SBQueueAdapter\\BizTalk.Adapter.SBQueueAdmin.dll"


